/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not(in=sel, out=notsel1);
    Not(in=notsel1, out=notsel2);
    And(a=in, b=notsel1, out=a);
    And(a=notsel2, b=in, out=b);
}
